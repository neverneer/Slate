{
  "name": "@slate/api",
  "version": "1.0.0",
  "description": "Slate API Service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "db:migrate": "node -r tsx/cjs scripts/migrate.ts",
    "db:seed": "node -r tsx/cjs scripts/seed.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.0.4",
    "supertest": "^6.3.3",
    "tsx": "^4.7.0",
    "vitest": "^1.0.4"
  }
}
